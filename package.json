{
    "name": "discord_lost_ark_community_bot",
    "version": "1.0.0",
    "main": "./dist/index.js",
    "license": "MIT",
    "repository": "",
    "author": "",
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "scripts": {
        "start-dev": "ts-node-dev --respawn --transpile-only ./index.ts",
        "start-cluster": "node ./node_modules/pm2/bin/pm2 start ecosystem.config.js --env production",
        "start-test": "ts-node-dev --respawn --transpile-only ./src/test/index.ts",
        "build": "rimraf ./dist && tsc -p .",
        "postversion": "git push --tags && git push && echo \"Successfully released version $npm_package_version!\"",
        "preversion": "yarn gitprune",
        "livepatch": "yarn version --patch",
        "liveminor": "yarn version --minor",
        "livemajor": "yarn version --major",
        "prerelease": "yarn version --prerelease --preid rc",
        "prepatch": "yarn version --prepatch --preid rc",
        "preminor": "yarn version --preminor --preid rc",
        "premajor": "yarn version --premajor --preid rc",
        "gitprune": "git config fetch.prune true && git config fetch.pruneTags true && git fetch",
        "ncu": "ncu",
        "ncu-u": "ncu -u",
        "postinstall": "yarn prisma generate"
    },
    "devDependencies": {
        "@types/app-root-path": "^1.2.4",
        "@types/bcryptjs": "^2.4.2",
        "@types/cors": "^2.8.12",
        "@types/node": "v16",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/parser": "^5.43.0",
        "cpy-cli": "^4.2.0",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "npm-check-updates": "^16.4.1",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "@bits_devel/api-error2": "^4.0.1",
        "@bits_devel/de-serializer": "^5.0.0",
        "@bits_devel/logger": "^2.0.1",
        "@bits_devel/swaggerhub-parser2": "^3.0.0",
        "@prisma/client": "^4.6.1",
        "app-root-path": "^3.1.0",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "discord.js": "^14.6.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-openapi-validator": "^4.13.8",
        "helmet": "^6.0.0",
        "pm2": "^5.2.2",
        "prisma": "^4.6.1",
        "swagger-ui-express": "^4.6.0",
        "tslib": "^2.4.1",
        "uuid": "^9.0.0"
    }
}
